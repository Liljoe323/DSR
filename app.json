{
  "expo": {
    "name": "DSR",
    "slug": "DSR",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/splash-icon.png",
    "scheme": "dsr",
    "extra": {
      "supabase": {
        "redirectUrl": "dsr://auth/callback",
        "url": "https://mozaczdffhifbqqcgaws.supabase.co",
        "anonKey": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6Im1vemFjemRmZmhpZmJxcWNnYXdzIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NDg3MjQ1MDIsImV4cCI6MjA2NDMwMDUwMn0.vew4V_GfthSxO5iYrizEivUHjqnSQ0v9pMi0rDngrWs"
      },
      "router": {},
      "eas": {
        "projectId": "6e684fb4-ee1d-4a1c-9743-3b00a76cde87"
      }
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

  
    "plugins": [
      "expo-router",
      "expo-image-picker",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.liljoe323.DSR",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSUserNotificationUsageDescription": "This app uses notifications to alert you of important updates.",

 
        "NSCameraUsageDescription": "We need camera access so you can take pictures for service requests.",
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to attach images to service requests.",
        "NSPhotoLibraryAddUsageDescription": "We need to save photos you take in your library for service requests."
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.liljoe323.DSR",


      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "experiments": {
      "typedRoutes": true
    }
  }
}
